{
  "name": "gallerium",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha ./src/**/**.test.js --require babel-polyfill --require babel-register -R spec --timeout 50000 --exit",
    "test:travis": "export NODE_ENV=test && npm test",
    "test:local": "set NODE_ENV=test&& npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "build": "babel src/Server -d src/build",
    "start:dev": "set NODE_ENV=development&& nodemon --exec babel-node src/Server/index",
    "lint": "eslint **/**/*.js --fix",
    "prettify:scripts": "prettier-eslint **/**/*.{js,json} --write",
    "prettify:styles": "prettier-stylelint **/**/*.css --write",
    "prettify:templates": "prettier **/**/*.html --write",
    "prettify:all": "npm run prettify:scripts && npm run prettify:styles && npm run prettify:templates"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.13",
    "multer": "^1.4.1",
    "socket.io": "^2.3.0",
    "web-push": "^3.3.5"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint-config-standard": "^18.3.0"
  }
}
